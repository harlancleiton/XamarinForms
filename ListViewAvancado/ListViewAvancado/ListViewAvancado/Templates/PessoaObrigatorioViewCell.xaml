        <StackLayout VerticalOptions="Center" Orientation="Horizontal">
            <Label Text="{Binding Nome}" />
            <Label Text=" - Rank: " />
            <Label Text="{Binding RankEficiencia}" />
        </StackLayout>
    </ViewCell.View>
</ViewCell>